{% macro AppNav(TargetHome, TargetSearch, TargetCart, TargetProfile) %}
    <div class="AppNav">
        <div class="AppNav_Container">
            <a href="{{ TargetHome }}" class="AppNav_Button">
                <span class="AppNav_Icon">{{ NavIcon('Home') }}</span>
                <span class="AppNav_IconClicked">{{ NavIcon('Home-filled') }}</span>
            </a>
            <a href="{{ TargetSearch }}" class="AppNav_Button">
                <span class="AppNav_Icon">{{ NavIcon('Search') }}</span>
                <span class="AppNav_IconClicked">{{ NavIcon('Search-filled') }}</span>
            </a>
            <a href="{{ TargetCart }}" class="AppNav_Button">
                <span class="AppNav_Icon">{{ NavIcon('Cart') }}</span>
                <span class="AppNav_IconClicked">{{ NavIcon('Cart-filled') }}</span>
            </a>
            <a href="{{ TargetProfile }}" class="AppNav_Button">
                <span class="AppNav_Icon">{{ NavIcon('Profile') }}</span>
                <span class="AppNav_IconClicked">{{ NavIcon('Profile-filled') }}</span>
            </a>
        </div>
    </div>
{% endmacro %}

